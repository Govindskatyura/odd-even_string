n= int(input())
arr=[]
for i in range(0,n):
    arr.append(input())
for i in range(0,len(arr)):
    odd=[]
    even=[]
    for j in range(0,len(arr[i])):
        if(j==0 or j%2==0):
            even.append(arr[i][j])
        else:
            odd.append(arr[i][j])
    print(''.join(even),end=' ',sep= ' ' )
    print(''.join(odd),end=' ',sep= ' ' )
    print()
